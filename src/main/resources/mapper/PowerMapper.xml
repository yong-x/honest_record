<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xy.honest_record.mapper.PowerMapper">


    <select id="getPowerById" parameterType="java.lang.Integer" resultType="Power">
        select p_id,p_name,pp_id,create_time,update_time from power where p_id=#{pid}
    </select>
</mapper>
